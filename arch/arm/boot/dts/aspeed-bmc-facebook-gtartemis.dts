// To PDB
&i2c8 {
	status = "okay";
	timeout;
	i2c-switch@72 {
		compatible = "nxp,pca9545";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x72>;
		imux36: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
			max11617_vpdb:adc@35 {
				compatible = "maxim,max11617";
				reg = <0x35>;
				#io-channel-cells = <1>;
			};
			ads1015_vpdb:adc@48 {
				compatible = "maxim,ads1015";
				reg = <0x48>;
				#io-channel-cells = <1>;
			};
		};

		imux37: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
			status = "okay";
			i2c-switch@71 {
				compatible = "nxp,pca9543";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x71>;
				imux40: i2c@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;
					fan_bp0_led: pca9552@62 {
						compatible = "nxp,pca9552";
						reg = <0x62>;
						#address-cells = <1>;
						#size-cells = <0>;
						gpio-controller;
						#gpio-cells = <2>;
						gpio@0 {
							reg = <0>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@1 {
							reg = <1>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@2 {
							reg = <2>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@3 {
							reg = <3>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@4 {
							reg = <4>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@5 {
							reg = <5>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@6 {
							reg = <6>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@7 {
							reg = <7>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@8 {
							reg = <8>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@9 {
							reg = <9>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@10 {
							reg = <10>;
							type = <PCA955X_TYPE_GPIO>;
						};
						gpio@11 {
							reg = <11>;
							type = <PCA955X_TYPE_GPIO>;
