	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "pnor";
		spi-max-frequency = <12000000>;
		spi-tx-bus-width = <2>;
		spi-rx-bus-width = <2>;
	};
};

&kcs2 {
	kcs_addr = <0xca8>;
	status = "okay";
};

&kcs3 {
	kcs_addr = <0xca2>;
	status = "okay";
};

&lpc_pcc {
	rec-mode = <0x1>;
	port-addr = <0x80>;
	port-addr-hbits-select = <0x1>;
	port-addr-xbits = <0x3>;
	status = "okay";
};

//eSPI
&espi_ctrl {
	status = "okay";

	perif,memcyc-src-addr = <0x98000000>;
	perif,memcyc-size = <0x10000>;
	perif,dma-mode;

	oob,dma-mode;
	oob,dma-tx-desc-num = <0x2>;
	oob,dma-rx-desc-num = <0x8>;

	flash,dma-mode;
	flash,safs-mode = <0x2>;
};

&peci0 {
	status = "okay";
	clock-frequency = <1000000>;
};

&sgpiom0 {
	status = "okay";
	gpio-ranges = <&pinctrl 0 244 128>;
	ngpios = <128>;
	base = <244>;
	bus-frequency = <2000000>;
};

&i2c0 {
	status = "okay";
	// CPU0
	sbrmi@3C {
		compatible = "amd,sbrmi";
		reg = <0x3C>;
	};
	sbtsi@4C {
		compatible = "amd,sbtsi";
		reg = <0x4C>;
	};
	// CPU1
	sbrmi@38 {
		compatible = "amd,sbrmi";
		reg = <0x38>;
	};
	sbtsi@48 {
		compatible = "amd,sbtsi";
		reg = <0x48>;
	};
};

&i2c1 {
	status = "okay";
	i2c-switch@70 {
		compatible = "nxp,pca9548";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x70>;
		imux20: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
			adc128d818@1d {
				compatible = "ti,adc128d818";
				reg = <0x1d>;
				ti,mode = /bits/ 8 <1>;
			};
			max11617_adc0:adc@35 {
				compatible = "maxim,max11617";
				reg = <0x35>;
				#io-channel-cells = <1>;
			};
		};
		imux21: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
		imux22: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
		};
		imux23: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;
		};
		imux24: i2c@4 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <4>;
		};
		imux25: i2c@5 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <5>;
		};
		imux26: i2c@6 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <6>;
			adc128d818@1d {
				compatible = "ti,adc128d818";
				reg = <0x1d>;
				ti,mode = /bits/ 8 <1>;
			};
			max11617_adc1:adc@35 {
				compatible = "maxim,max11617";
				reg = <0x35>;
				#io-channel-cells = <1>;
			};
		};
		imux27: i2c@7 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <7>;
		};
	};
	tmp75@4B {
		compatible = "ti,tmp75";
		reg = <0x4B>;
	};
};

&i2c2 {
	status = "okay";
};

//To ACB BIC
&i2c3 {
	status = "okay";
	multi-master;
	/delete-property/ buff-mode;
	byte-mode; 
	bus-frequency = <400000>;
	timeout;
	base-clock-divisor = <2>;
	tck-high = <8>;
	tck-low = <14>;
};

&i2c4 {
	status = "okay";
	multi-master;
	/delete-property/ buff-mode;
	byte-mode; 
	// NIC1 FRU
	eeprom@52 {
		compatible = "atmel,24c64";
		reg = <0x52>;
	};
	tmp421@1F {
		compatible = "ti,tmp421";
		reg = <0x1F>;
	};
};

