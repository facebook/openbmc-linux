							#address-cells = <1>;
							#size-cells = <0>;
							reg = <3>;
						};
						imux75: i2c@4 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <4>;
						};
						imux76: i2c@5 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <5>;
						};
						imux77: i2c@6 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <6>;
						};
						imux78: i2c@7 {
							#address-cells = <1>;
							#size-cells = <0>;
							reg = <7>;
						};
					};				
				};
				imux69: i2c@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;	
				};
				imux70: i2c@2 {
					#address-cells = <1>;
